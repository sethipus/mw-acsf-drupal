<?php

/**
 * @file
 * Update scripts for the Mars Product module.
 */

/**
 * Implements hook_update_N() for rating&reviews per site for all products (turned on by default).
 */
function mars_product_update_8901() {
  \Drupal::configFactory()
    ->getEditable('emulsifymars.settings')
    ->set('show_rating_and_reviews', TRUE)
    ->save();
}
