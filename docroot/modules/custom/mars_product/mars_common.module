use Drupal\Core\Form\FormStateInterface;

/**
 * Implements hook_form_alter().
 */
function mars_common_form_alter(&$form, FormStateInterface $form_state, $form_id) {
  if ($form_id === 'block_form') {
    unset($form['settings']['label_display']);
  }
}
