<!-- Error Page Header -->
{% if theme_styles == 'dove' %}
  {% include '@organisms/site/site-header/site-header.twig' with {
    alert_banner:'Give the gift of pleasure this Valentine’s Day with the DOVE Signature Collection',
    logo_src: 'https://www.dovechocolate.com/sites/g/files/fnmzdf186/files/dove-chocolate-us-logo.svg'
  } %}
{% elseif theme_styles == 'twix' %}
  {% include '@organisms/site/site-header/site-header.twig' with {
     alert_banner:'Give the gift of pleasure this Valentine’s Day with the TWIX Signature Collection',
    logo_src: 'twix-logo.png'
  } %}
{% elseif theme_styles == 'galaxy' %}
  {% include '@organisms/site/site-header/site-header.twig' with {
     alert_banner:'Give the gift of pleasure this Valentine’s Day with the GALAXY Signature Collection',
    logo_src: 'https://www.galaxychocolate.co.uk/sites/g/files/fnmzdf211/files/galaxy.svg'
  } %}
{% elseif theme_styles == 'mars' %}
  {% include '@organisms/site/site-header/site-header.twig' with {
     alert_banner:'Give the gift of pleasure this Valentine’s Day with all our MARS chocolates Signature Collection',
    logo_src: 'mars_logo.png'
  } %}
{% else %}
  {% include '@organisms/site/site-header/site-header.twig' %}
{% endif %}

{% include '@molecules/article-header/article-header-image.twig' %}


<!-- Zone 2 -  Article Body -->
{% if StoryTellingOption == 'Recipe Feature' %}
  {% include '@molecules/recipe-feature-module/recipe-feature-module.twig' %}
{% elseif StoryTellingOption == 'Full Width Media' %}
  {% include '@molecules/article-media/full-width/full-width-media.twig' with {
    paragraph_modifiers:''
  }%}
{% elseif StoryTellingOption == 'Flexible Framer' %}
  {% include '@molecules/flexible-framer/flexible-framer.twig' %}
{% elseif StoryTellingOption == 'Flexible Driver' %}
  {% include '@molecules/flexible-driver/flexible-driver.twig' %}
{% elseif StoryTellingOption == 'List' %}
  {% include '@molecules/article-media/list/article-list.twig' %}
{% elseif StoryTellingOption == 'Contact & Help Banner' %}
  {% include '@molecules/contact-module/contact-module.twig' %}
{% elseif StoryTellingOption == 'WYSIWYG' %}
  {% include '@molecules/article-media/wysiwyg/article-wysiwyg.twig' with {
    paragraph_modifiers:''
  } %}
{% elseif StoryTellingOption == 'Content Feature' %}
  {% include '@molecules/content-feature/content-feature.twig' %}
{% elseif StoryTellingOption == 'Feedback Module' %}
  {% include '@molecules/feedback-module/feedback.twig' %}
{% elseif StoryTellingOption == 'Story Highlight' %}
   {% include '@molecules/story-highlight/story_highlight.twig' with {
    heading_level: 2
  } %}
{% elseif StoryTellingOption == 'Polls' %}
  {% include '@molecules/polls/poll-vote.twig' %}
{% else %}
  {% include '@atoms/iframe/iframe.twig' %}
{% endif %}
<!-- Zone 2 Ends -->




