<!-- Error Page Header -->
{% if theme_styles == 'dove' %}
  {% include '@organisms/site/site-header/site-header.twig' with {
    logo_src: 'https://www.dovechocolate.com/sites/g/files/fnmzdf186/files/dove-chocolate-us-logo.svg'
  } %}
{% elseif theme_styles == 'twix' %}
  {% include '@organisms/site/site-header/site-header.twig' with {
    logo_src: 'twix-logo.png'
  } %}
{% elseif theme_styles == 'galaxy' %}
  {% include '@organisms/site/site-header/site-header.twig' with {
    logo_src: 'https://www.galaxychocolate.co.uk/sites/g/files/fnmzdf211/files/galaxy.svg'
  } %}
{% elseif theme_styles == 'mars' %}
  {% include '@organisms/site/site-header/site-header.twig' with {
    logo_src: 'mars_logo.png'
  } %}
{% else %}
  {% include '@organisms/site/site-header/site-header.twig' %}
{% endif %}

<!-- Zone 1 - Parent Page Header -->
{% include '@molecules/parent-page-header/parent-page-header.twig' %}
<!-- Zone 1 Ends -->

<!-- Zone 2 - Featured Content -->
{% if feature_content_option == 'Product Feature Module' %}
  {% include '@molecules/product-feature/product-feature.twig' %}
{% elseif feature_content_option == 'Content Feature Module' %}
  {% include '@molecules/content-feature/content-feature.twig' %}
{% elseif feature_content_option == 'Recipe Feature Module' %}
  {% include '@molecules/recipe-feature-module/recipe-feature-module.twig' %}
{% else %}
  {% include '@molecules/product-content-pair-up/product-content-pair-up.twig' %}
{% endif %}
<!-- Zone 2 Ends -->

<!-- Zone 5 - Driver Content -->
{% if driver_content_option == 'Flexible Driver' %}
  {% include '@molecules/flexible-driver/flexible-driver.twig' %}
{% else %}
   {% include '@molecules/flexible-framer/flexible-framer.twig' %}
{% endif %}
<!-- Zone 5 Ends -->

<!-- Zone 6 - Community Engagement -->
{% if community_option == 'Feedback Module' %}
  {% include '@molecules/feedback-module/feedback.twig' %}
{% else %}
   {% include '@molecules/polls/poll-vote.twig' %}
{% endif %}
<!-- Zone 5 Ends -->


<!-- Error Page Footer -->
{% if theme_styles == 'dove' %}
  {% include '@organisms/site/site-footer/site-footer.twig' with {
    logo_src: 'https://www.dovechocolate.com/sites/g/files/fnmzdf186/files/dove-chocolate-us-logo.svg'
  } %}
{% elseif theme_styles == 'twix' %}
  {% include '@organisms/site/site-footer/site-footer.twig' with {
    logo_src: 'twix-logo.png'
  } %}
{% elseif theme_styles == 'galaxy' %}
  {% include '@organisms/site/site-footer/site-footer.twig' with {
    logo_src: 'https://www.galaxychocolate.co.uk/sites/g/files/fnmzdf211/files/galaxy.svg'
  } %}
{% elseif theme_styles == 'mars' %}
  {% include '@organisms/site/site-footer/site-footer.twig' with {
    logo_src: 'mars_logo.png'
  } %}
{% else %}
  {% include '@organisms/site/site-footer/site-footer.twig' %}
{% endif %}
