{% embed "@molecules/article-header/article-header-image.twig" with {
  theme_styles: 'drupal',
  shape: brand_borders,
  eyebrow: eyebrow,
  heading: label,
  date_published: publication_date,
  share_text: 'Share'|t
} %}
  {% block article_media_image %}
    {% include "@atoms/images/image/responsive-image.twig" with {
      output_image_tag: true,
      image_alt: image.label,
      image_srcset: image.url,
      image_sizes: '(max-width: 375px) 375px,
                (max-width: 768px) 768px,
                1440px',
      image_src: image.url,
      responsive_image_blockname: 'article-header-image',
    } %}
  {% endblock %}
  {% block article_media_devider %}
    <div class="{{ brand_shape_class }}">
      {{ shape | raw }}
    </div>
  {% endblock %}
  {% block article_social_menu %}
    {% include "@molecules/menus/social/social-menu.twig" with {
      social_menu_items: social_links,
      configured_icons: true
    } %}
  {% endblock %}
{% endembed %}
