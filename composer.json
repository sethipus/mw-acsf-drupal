    "require-dev": {
        "behat/mink-selenium2-driver": "^1.6",
        "drupal/core-dev": "^9.3",
        "mikey179/vfsstream": "^1.6",
        "phpspec/prophecy-phpunit": "^2",
        "symfony/phpunit-bridge": "^5.3"
    },
    "config": {
        "platform": {
            "php": "8.0"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "drupal/core-vendor-hardening": true,
            "acquia/blt": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "oomphinc/composer-installers-extender": true,
            "simplesamlphp/composer-module-installer": true
        }
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "drupal-scaffold": {
            "locations": {
                "web-root": "./docroot"
            },
            "allowed-packages": {
                "drupal/core": "*"
            },
            "file-mapping": {
                "[web-root]/.htaccess": false
            },
            "excludes": [
                ".htaccess"
            ],
            "gitignore": false
        },
        "enable-patching": true,
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/core": {
                "2321071 - Base field overrides fix": "https://www.drupal.org/files/issues/2019-12-24/2321071-39.patch",
                "2925297 - Fatal error on config form with translation enabled when config is missing": "https://www.drupal.org/files/issues/2019-09-26/core-typed_config_handle_missing_config-2925297-18.patch",
                "2869592 - Disabled update module shouldn't produce a status report warning": "https://www.drupal.org/files/issues/2020-02-07/2869592-remove-update-warning-34.patch",
                "[subprofile support] 1356276 - Allow profiles to define a base/parent profile and load them in the correct order / 2914389 - Allow profiles to exclude dependencies of their parent": "https://www.drupal.org/files/issues/2021-11-18/1356276-675.patch"
            },
            "drupal/config_ignore": {
                "Correct drush filtering": "https://www.drupal.org/files/issues/2021-01-04/config_ignore_2857247-61.patch",
                "Config ignore collection": "https://www.drupal.org/files/issues/2020-11-04/config-ignore-collections-2973431-57.patch"
            },
            "drupal/tmgmt": {
                "The job has no provider assigned": "https://www.drupal.org/files/issues/2020-10-30/tmgmt-2690201-19.patch",
                "Source list limit setting": "https://www.drupal.org/files/issues/2020-11-11/tmgmt-settings-limit.patch"
            },
            "drupal/jquery_colorpicker": {
                "Fixed jquery colorpicker security issue": "patches/Fixed-jquery-colorpicker-security-issue.patch",
                "Set a high z-index on colorpicker widget": "https://www.drupal.org/files/issues/2020-09-10/increase-z-index-value-3058389-6.patch"
            },
            "drupal/field_group": {
                "Ensure visibility of invalid fields (JS) https://www.drupal.org/project/field_group/issues/2787179": "https://www.drupal.org/files/issues/2020-05-19/2787179-highlight-html5-validation-52.patch"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Acquia\\Blt\\Overrides\\": "blt/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Tests\\PHPUnit\\": "tests/phpunit/src/"
        }
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "blt-azure-pipelinest": {
            "type": "git",
            "url": "https://github.com/mikemadison13/blt-azure-pipelines"
        },
        "jquery_colorpicker": {
            "type": "package",
            "package": {
                "name": "jaypan/jquery-colorpicker",
                "version": "1.0.1",
                "type": "drupal-library",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/jaypan/jquery_colorpicker/archive/refs/tags/1.0.1.zip"
                }
            }
        },
        "colorbutton": {
            "type": "package",
            "package": {
                "name": "ckeditor/colorbutton",
                "version": "master",
                "type": "drupal-library",
                "dist": {
                    "type": "zip",
                    "url": "https://download.ckeditor.com/colorbutton/releases/colorbutton_4.16.2.zip",
                    "reference": "master"
                },
                "autoload": {
                    "classmap": ["."]
                }
            }
        },
        "panelbutton": {
            "type": "package",
            "package": {
                "name": "ckeditor/panelbutton",
                "version": "master",
                "type": "drupal-library",
                "dist": {
                    "type": "zip",
                    "url": "https://download.ckeditor.com/panelbutton/releases/panelbutton_4.16.2.zip",
                    "reference": "master"
                },
                "autoload": {
                    "classmap": ["."]
                }
            }
        },
        "mars_seo": {
            "type": "path",
            "url": "docroot/modules/custom/mars_seo"
        },
        "brand_drupal_chats": {
            "type": "package",
            "package": {
                "name": "mars/brand_drupal_chats",
                "version": "1.0.6",
                "type": "drupal-module",
                "source": {
                    "url": "git@github.com:Mars-Incorporated/brand_drupal_chats.git",
                    "type": "git",
                    "reference": "1.0.6"
                }
            }
        },
        "mars_lighthouse": {
            "type": "package",
            "package": {
                "name": "mars/mars_lighthouse",
                "version": "v0.0.2",
                "type": "drupal-module",
                "source": {
                    "url": "git@github.com:Mars-Incorporated/mars_lighthouse.git",
                    "type": "git",
                    "reference": "v0.0.2"
                }
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts-descriptions": {
        "mw:acsf:validate": "Validates php, twig and yaml.",
        "mw:acsf:blt:validate": "BLT deploy validation.",
        "mw:fossa": "Run a FOSSA scan."
    },
    "scripts": {
        "mw:acsf:validate": [
            "blt validate:php --ansi",
            "blt validate:twig --ansi",
            "blt validate:yaml --ansi"
        ],
        "mw:acsf:blt:validate": [
            "vendor/bin/blt validate --define drush.alias='$(drush.aliases.ci)' --ansi --verbose"
        ],
        "mw:fossa": [
            "FOSSA_API_KEY=028db647245bf9493f174c2261bd9736 fossa analyze"
        ],
        "post-create-project-cmd": [
            "blt internal:create-project:init-repo"
        ],
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "nuke": [
            "rm -rf vendor composer.lock docroot/core docroot/modules/contrib docroot/profiles/contrib docroot/themes/contrib",
            "@composer clearcache --ansi",
            "@composer install --ansi"
        ],
        "post-install-cmd": [
            "rm docroot/robots.txt"
        ],
        "post-update-cmd": [
            "rm docroot/robots.txt"
        ]
    }
}
