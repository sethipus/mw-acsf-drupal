        "drupal/core-vendor-hardening": "^9.3",
        "drupal/csp": "^1.16",
        "drupal/datalayer": "^1.0@beta",
        "drupal/dimension": "^2.0",
        "drupal/editor_advanced_link": "^2.0",
        "drupal/entity_embed": "^1.0",
